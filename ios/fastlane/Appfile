app_identifier("template.app") # The bundle identifier of your app
apple_id(ENV['FASTLANE_APPLE_USER']) # Your Apple Developer Portal username

itc_team_name(ENV['FASTLANE_template_ITC_TEAM_NAME']) # App Store Connect Team Name
itc_team_id(ENV['FASTLANE_template_ITC_TEAM_ID']) # App Store Connect Team ID
dev_portal_team_id(ENV['FASTLANE_template_DEV_PORTAL_TEAM_ID']) # Developer Portal Team ID

